, collection) => {\n    \n    acc [collection.title.toLowerCase()] = collection\n\n    return acc\n}, { })\n}\n\nexport const getCurrentUser = () => {\n\n    return new Promise((resolve, reject) => {\n\n           const unsubscribe = auth.onAuthStateChanged(userAuth => {\n\n               unsubscribe()\n               resolve(userAuth)\n           }, reject)\n    })\n}\n\n// Initialize Firebase\n\nfirebase.initializeApp(config)\n\nexport const auth = firebase.auth()\nexport const firestore = firebase.firestore()\n\nexport const googleProvider = new firebase.auth.GoogleAuthProvider()\ngoogleProvider.setCustomParameters({prompt: 'select_account'})\n\nexport const signInWithGoogle = () => auth.signInWithPopup(googleProvider)\n\nexport default firebase","/home/anaruz/www/react/crwn-cloth-learning/client/src/redux/directory/directory.selectors.js",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/pages/collection/collection.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/collections-overview/collections-overview.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/form-input/form-input.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/custom-button/custom-button.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/menu-item/menu-item.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/with-spinner/with-spinner.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/cart-item/cart-item.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/collections-overview/collections-overview.styles.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/pages/collection/collection.styles.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/custom-button/custom-button.styles.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/clien