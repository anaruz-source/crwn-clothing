th-learning/client/src/components/sign-in/sign-in.component.jsx",["248"],"import React, { useState, useEffect } from 'react'\nimport { connect } from 'react-redux'\n\nimport FormInput from '../form-input/form-input.component'\nimport CustomButton from '../custom-button/custom-button.component'\n\n\nimport { googleSignInStart, emailSignInStart } from './../../redux/user/user.actions'\n\nimport './sign-in.styles.scss'\n\n\nconst SignIn = ({ emailSignInStart, googleSignInStart}) => {\n\n\n      const [credentials, setCredentials] = useState({\n\n        email: '',\n        password: ''\n      })\n\n      const { email, password } = credentials\n\n    const handleSubmit = e => {\n\n        e.preventDefault()\n\n\n        emailSignInStart(email, password )\n    }\n\n   const handleChange = e => {\n       const {name, value} = e.target\n\n      setCredentials({...credentials, [name]: value})\n    \n    }\n\n\n      return (  <div className='sign-in'>\n\n             <h2>I already have an account</h2>\n             <span>Sign in with your email and password</span>\n\n          <form onSubmit= { handleSubmit }>\n\n                 <FormInput type='email' name='email' id='email' value={email} handleChange = {handleChange} label='email' required/>\n                \n\n                <FormInput type='password' name='password' id='password' value={password} handleChange={handleChange}  label='password' required />\n                \n                <div className='buttons'>\n\n\n                  <CustomButton type='submit' >Sign In</CustomButton>\n                  <CustomButton type='button' onClick={googleSignInStart} isGoogleSignIn>Sign In With Google</CustomButton>\n                </div>\n\n             </form>\n        </div>\n      )\n}\n\nconst mapDispatchToProps = dispatch =>({\n\n    googleSignInStart: () => dispatch(googleSignInStart()),\n    emailSignInStart: (email, password) => dispatch(emailSignInStart({ email, password}))\n})\n\nexport default connect(null, mapDispatchToProps)(SignIn)","/home/anaruz/www/react/crwn-cloth-learning/client/src/components/sign-up/sign-up.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/directory/directory.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/pages/collection/collection.container.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/collections-overview/collections-overview.container.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/checkout-item/checkout-item.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/stripe-button/stripe-button.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/cart-icon/cart-icon.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/cart-dropdown/cart-dropdown.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/redux/shop/shop.types.js",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/redux/cart/cart.actions.js",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/redux/cart/cart.types.js",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/redux/cart/cart.utils.js",["249"],"import cartItem from \"../../components/cart-item/cart-item.component\"\n\n export const addItemToCart = (cartItems, cartItem) => {\n\n const exitingItem = cartItems.find(item =>  item.id === cartItem.id)\n\n   if(exitingItem) {\n              \n      return cartItems.map (item =>  item.id === cartItem.id ? {...item, quantity: item.quantity + 1 } : item)\n   }\n\n  return [...cartItems, {...cartItem, quantity: 1}]\n\n} \n\nexport const removeItemFromCart = (items, item) => {\n\n  const existingItem = items.find( elem => elem.id === item.id)\n\n  if(existingItem.quantity === 1) {\n\n    return items.filter(elem => elem.id !== item.id)\n  }\n\n  return items.map (elem => elem.id === item.id ? {...elem, quantity: elem.quantity -1} : elem)\n}\n","/home