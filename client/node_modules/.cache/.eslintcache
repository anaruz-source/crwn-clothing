/collections-overview.container'\n\n\n\nconst   ShopPage = ({ fetchCollectionsStart, match}) => {\n         \n\n\n    useEffect(() => {\n\n        \n\n        fetchCollectionsStart()\n\n    },[])\n\n\n    \n\n          return  ( <div className = 'shop-page' >\n\n              <Route exact path={`${match.path}`}  component = { CollectionOverviewContainer}  /> \n              <Route path={`${match.path}/:collectionId`} component = { CollectionPageContainer } />\n           \n           </div> )\n\n\n       \n    }\n\n\nconst mapDispatchToProps = dispatch => ({\n         \n    fetchCollectionsStart: () => dispatch(fetchCollectionsStart())\n})\n\n\nexport default connect(null, mapDispatchToProps)(ShopPage)","/home/anaruz/www/react/crwn-cloth-learning/client/src/pages/checkout/checkout.component.jsx",[],"/home/anaruz/www/react/crwn-cloth-learning/client/src/components/header/header.component.jsx",["236"],"import React from 'react'\nimport { connect } from 'react-redux';\nimport {createStructuredSelector} from 'reselect'\n\n\nimport {ReactComponent as Logo} from '../../assets/crown.svg'\nimport CartIcon from '../cart-icon/cart-icon.component'\nimport CartDropdown from '../cart-dropdown/cart-dropdown.component'\n\nimport {HeaderContainer, LogoContainer, OptionsContainer, OptionLink, OptionDiv } from './header.styles'\n\nimport {selectCartHidden} from '../../redux/cart/cart.selectors'\nimport {selectCurrentUser} from '../../redux/user/user.selectors'\nimport { signOutStart } from './../../redux/user/user.actions'\n\n\nconst Header = ({ currentUser, hidden, signOutStart }) => (\n\n   <HeaderContainer>\n        <LogoContainer to='/'>\n            <Logo className='logo'/>\n        </LogoContainer>\n        <OptionsContainer>\n            <OptionLink  to='/shop'>SHOP</OptionLink>\n            <OptionLink to='/contact'>CONTACT</OptionLink>\n            \n            { currentUser ? \n            \n                (<OptionLink as = 'div' onClick={() => signOutStart()}>SIGN OUT</O